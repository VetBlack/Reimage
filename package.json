{
  "name": "reimage",
  "version": "1.0.9",
  "description": "React library for comfortable and ease work with images.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" \"src/**/*.tsx\"",
    "lint": "tslint -p tsconfig.json",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VetBlack/Reimage.git"
  },
  "keywords": [
    "react",
    "image",
    "images",
    "picture",
    "lazy",
    "load",
    "reactjs",
    "img",
    "image",
    "loader",
    "fallback",
    "placeholder",
    "image placeholder"
  ],
  "files": [
    "*.md",
    "es/*.js",
    "esm/*.js",
    "umd/*.js",
    "cjs/*.js",
    "!**/*.test.js",
    "*.d.ts"
  ],
  "author": "Vitalii Chernyavskyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VetBlack/Reimage/issues"
  },
  "homepage": "https://github.com/VetBlack/Reimage#readme",
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 10 chrome version",
      "last 10 firefox version",
      "last 2 safari version",
      "last 1 ie version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "jest": "^29.7.0",
    "prettier": "^1.19.1",
    "release-it": "^15.6.0",
    "rollup": "^2.7.6",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-terser": "^7.0.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}